    <!-- <h1>Courses Details</h1> -->
    <div class="wrapper">
      <% Object.keys(courseDetail).forEach(function(course){ %>
      <div class="course"><h2><%-courseDetail[course].title %></h2></div>
      <div class="course"><%-courseDetail[course].id%></div>
      <div class="course"><%-courseDetail[course].price%></div>
      <div class="course"><%-courseDetail[course].price_detail.currency%></div>
      <div class="course"><%-courseDetail[course].is_paid%> <% }) %></div>
    </div>
